
%%% Algorithmic modifications %%%%

% \renewcommand{\algorithmicrequire}{\textbf{Input:}}
% \renewcommand{\algorithmicensure}{\textbf{Output:}}


%affine hull
% \aff(X)
\DeclareMathOperator{\aff}{aff}


% Bold math letters
% \mbf{f} \in \mathcal{F}
\newcommand*{\mbf}[1]{\mathbf{#1}}

% unit ball
% \uball
\newcommand*{\uball}{\mathbb{B}}




% closure operator
% \cl(X)
\DeclareMathOperator{\cl}{cl}

% "Conditioned on" bar
\newcommand*{\cond}{\,|\,}

%cone generated by
% \cone(X)
\DeclareMathOperator{\cone}{cone}

% convex hull
% \conv(X)
\DeclareMathOperator{\conv}{conv}

% Infimal convolution
% f \infconv g
\DeclareMathOperator{\infconv}{\Box}

% curly braces
% \curly{x}
% \curly[\Bigg]{\sum_{i \in I} x_i}
% \curly*{\sum_{i \in I} x_i} uses \left and \right
\DeclarePairedDelimiter\curly{\{}{\}}

% Diagonal matrix induced by a vector
% \Diag(x)_{i,i} = x_i
\DeclareMathOperator{\Diag}{Diag}

% Vector induced by the diagonal of a matrix
% \diag(X)_{i} = X_{i,i}
\DeclareMathOperator{\diag}{diag}

% Diff operator for integrals and derivatives
\newcommand*\diff{\mathop{}\!\mathrm{d}}


% domain of a function
% \dom f 
\DeclareMathOperator{\dom}{dom}

% the empty set
% \emptyset
\let\oldemptyset\emptyset
\renewcommand*{\emptyset}{\varnothing}

% the empty sequence
% \emptyset
\newcommand*{\emptyseq}{\angb{}}


% Eigenvalue vector in non-decreasing order
% \lambdaup(X)^{\transp} \ones
\newcommand*{\lambdaup}{\lambda^{\uparrow}}

% Epigraph of a function
% \epi f
\DeclareMathOperator{\epi}{epi}

% epsilon
% \eps
\newcommand*{\eps}{\varepsilon}

% euclidean space
% x \in \Euclid
\newcommand*{\Euclid}{\mathbb{E}}

% Hadamard product
% \hadamard
\newcommand*{\hadamard}{\mathbin{\circ}}

% Indicator function 
\newcommand{\indic}[1]{\ones_{#1}}


% interior of a set
% \interior U
\DeclareMathOperator{\interior}{int}

% set of integers
% \Integers
\newcommand*{\Integers}{\mathbb{Z}}

% inner product, <.,.> notation
% \iprod{I}{X}
\DeclarePairedDelimiter\angb{\langle}{\rangle}
\newcommand\iprod[3][]{\angb[#1]{#2,#3}}
\WithSuffix\newcommand\iprod*[2]{\angb*{#1,#2}}


% inner product, .^T. notation
% \iprodt{x}{y}
\newcommand{\iprodt}[2]{#1^{\transp}#2}

% Integer interval a, ... b
% \iinterval{a}{b}
\newcommand{\iinterval}[2]{\{#1, \dotsc ,#2\}}

% Iverson notation
% \boole{x > 0}
% \boole[\Big]{\sum_{i=1}^n x_i > 0}
% \boole*{\sum_{i=1}^n x_i > 0} uses \left and \right
\newcommand\boole[2][]{\mathbf{1}\sqbrac[#1]{#2}}
\WithSuffix\newcommand\boole*[1]{\sqbrac*{#1}}

% linear hull
% \lin(X)
\DeclareMathOperator{\lin}{lin}

% mathrm


% natural numbers
\newcommand{\Naturals}{\mathbb{N}}



% Requires: \usepackage{bbm}
% vector of all-ones
\newcommand*{\ones}{\mathbbm{1}}

% parentheses
% \paren{x}
% \paren[\Bigg]{\sum_{i \in I} x_i}
% \paren*{\sum_{i \in I} x_i} uses \left and \right
\DeclarePairedDelimiter\paren{(}{)}

% piped
% \piped{x}
% \piped[\Bigg]{\sum_{i \in I} x_i}
% \piped*{\sum_{i \in I} x_i} uses \left and \right
\DeclarePairedDelimiter\piped{|}{|}

% proximal mapping
% \prox_f(x)
% \prox\paren*{\frac{1}{\lambda} f(x)}
\DeclareMathOperator{\prox}{prox}

% Overline
% \ovl{\mathcal{P}}
\newcommand*{\ovl}[1]{\overline{#1}}

% power set
% \Powerset{V}
\newcommand*{\Powerset}[1]{\mathcal{P}(#1)}

% Probability 
% \Prob
\newcommand*{\Prob}{\mathbbm{P}}

% Set of symmetric n x n matrices
% X \in \Sym{n}
\newcommand*{\Sym}[1]{\mathbb{S}^{#1}}

% set of rationals
% \Rationals
\newcommand*{\Rationals}{\mathbb{Q}}

% set of reals
% \Reals
\newcommand*{\Reals}{\mathbb{R}}

% set of the extended reals
\newcommand*{\Extreals}{(-\infty, +\infty]}

% Improper extended reals
\newcommand*{\Impreals}{[-\infty, +\infty]}

% relative interior
% \relint(X)
\DeclareMathOperator{\relint}{ri}

% function restriction
% f\restrict{S}
\newcommand*{\restrict}[1]{\mathord{\restriction}_{#1}}


% set of sequences of elements of a set
% s \in \Seq(D)
\DeclareMathOperator{\Seq}{Seq}


% square brackets
% \sqbrac{x}
% \sqbrac[\Bigg]{\sum_{i \in I} x_i}
\newcommand{\sqbrac}[2][]{#1[{#2}#1]}

% set curly braces
% \set{1,2,\dotsc,n}
% \set[\Bigg]{\bigcup_{i \in I} A_i}
% \newcommand{\set}[2][]{#1\{#2#1\}}


% Subdifferential of a function
% \subdiff(x)
% \subdiff[g](x)
\newcommand\subdiff[1][]{\partial \ifthenelse{\equal{#1}{}}{f}{#1}}

% support
% \supp(x)
\DeclareMathOperator{\supp}{supp}


% Support function \suppf[K]{x} \suppf{x} (== \indic[C]{x})
\newcommand\suppf[2][]{\delta^*\paren{#2 \,\vert\,
    \ifthenelse{\equal{#1}{}}{C}{#1}}}

% Tilde big-O notation
% \tildeO(m)
\newcommand*{\tildeO}{\tilde{O}}

% Tilde big-Omega notation
% \tildeO(m)
\newcommand*{\tildeOmega}{\tilde{\Omega}}


% Trace
% \Tr(A)
\DeclareMathOperator{\Tr}{Tr}

% transpose
% X^{\transp}
\newcommand*{\transp}{\mathsf{T}}
\newcommand*{\T}{\mathsf{T}}


\newcommand{\jmlrBlackBox}{\rule{1.5ex}{1.5ex}}
\providecommand{\BlackBox}{\jmlrBlackBox}
\newcommand{\jmlrQED}{\hfill\jmlrBlackBox\par\bigskip}

%%%%%%%%%%%%%%% DP Related defs %%%%%%%%%%%%%%%

\newcommand*{\Mgauss}{\Mcal_{\mathrm{Gauss}}}
\newcommand*{\Mproj}{\Mcal_{\mathrm{Proj}}}


\DeclareMathOperator{\Gen}{Gen}
\DeclareMathOperator{\Trace}{Trace}


%%%%%%%% vech Defs %%%%%%%%%%%%

\DeclareMathOperator{\vech}{vech}
\renewcommand{\vec}{\mathrm{vec}}
\DeclareMathOperator{\sym}{sym}



